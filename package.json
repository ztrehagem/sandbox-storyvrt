{
  "name": "sandbox-storyvrt",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@10.16.0",
  "scripts": {
    "dev": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "playwright:ui": "playwright test --ui",
    "playwright:vrt": "playwright test --project 'vrt:*' --pass-with-no-tests",
    "playwright:vrt:update": "VRT_ALL=true playwright test --project 'vrt:*' --update-snapshots",
    "playwright:vrt:download": "gh run download --name playwright-vrt-snapshots --dir playwright-snapshots/vrt/",
    "eslint": "eslint .",
    "prettier": "prettier --ignore-path .gitignore --check .",
    "prettier.fix": "pnpm run prettier --write"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.55.0",
    "@storybook/addon-a11y": "^9.1.5",
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/addon-vitest": "^9.1.5",
    "@storybook/react-vite": "^9.1.5",
    "@types/node": "^24.3.3",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.1.5",
    "globals": "^16.3.0",
    "http-server": "^14.1.1",
    "playwright": "^1.55.0",
    "prettier": "^3.6.2",
    "storybook": "^9.1.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  }
}
